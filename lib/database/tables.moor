CREATE TABLE hydrationPlans (
    id INT,
    gender TEXT,
    birthday TEXT,
    wakeupTime TEXT,
    bedtime TEXT,
    dailyGoal INT,
    liquidMeasurement TEXT,
    isUsingRecommendedDailyGoal INT,
    joinedDate TEXT
);

createHydrationPlan: INSERT INTO hydrationPlans VALUES (1, ?, ?, ?, ?, ?, ?, ?, 
    ?);

readHydrationPlan: SELECT * FROM hydrationPlans;

deleteHydrationPlan: DELETE FROM hydrationPlans;